  var p = s.split("/");
  return p[p.length-1];
}
return null;
}
function getDOI() {
var d = $("#ddDoi").attr("href");
if(d) {
  return d.replace("http://dx.doi.org/", ""); } return null; } var ddWait=0; var ddNumWait=20; function buildDD() {
